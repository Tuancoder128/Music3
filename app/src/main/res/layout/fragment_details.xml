<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frame_detais"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f4b18e"
    tools:context="com.bkav.demo.music.DetailSongRuningActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include layout="@layout/layout_head_song" />

        <ImageView
            android:background="@drawable/shape_image_v"
            android:id="@+id/background_hinhalbum"
            android:layout_width="match_parent"
            android:layout_height="450dp"
             />

        <include

            android:id="@+id/includebottom"
            layout="@layout/layout_bottom_go" />
    </LinearLayout>


</FrameLayout>



